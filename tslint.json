{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      "single"
    ],
    "interface-name": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "prefer-const": true,
    "object-literal-sort-keys": false,
    "max-classes-per-file": [
      false
    ],
    "object-literal-key-quotes": [
      false
    ],
    "no-invalid-this": [
      true
    ],
    "no-switch-case-fall-through": true,
    "no-empty": [
      true,
      "allow-empty-catch"
    ],
    "member-ordering": false,
    "ordered-imports": false,
    "member-access": false
  }
}
